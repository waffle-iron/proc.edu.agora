{
  "name": "proc.edu.agora",
  "version": "1.0.0",
  "description": "A procedural game about enclosed space and survival",
  "main": "index.html",
  "scripts": {
    "watch": "node ./node_modules/cli-watch/index.js -f ./src/**/*.es6.js -r \"npm run build\"",
    "build": "node ./node_modules/browserify/bin/cmd.js ./src/main.es6.js -o ./dist/proc.edu.agora.js -d -t [ babelify --presets [ es2015 ] ]",
    "start": "node ./node_modules/http-server/bin/http-server -p 1337 ./",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagora/proc.edu.agora.git"
  },
  "keywords": [
    "procedural",
    "indie",
    "game",
    "rogue"
  ],
  "author": "franck @lagora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lagora/proc.edu.agora/issues"
  },
  "babel": {
    "plugins": "es2015",
    "sourceMaps": true
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "homepage": "https://github.com/lagora/proc.edu.agora#readme",
  "dependencies": {
    "async-waterfall": "^0.1.5",
    "babel-polyfill": "^6.8.0",
    "fs-extra": "^0.30.0",
    "md5": "^2.1.0",
    "querystringparser": "^0.1.1",
    "redux": "^3.5.2",
    "static-http-server": "0.0.5",
    "three": "^0.76.1",
    "three-orbit-controls": "^72.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.7",
    "browserify": "^13.0.0",
    "express": "^4.13.4",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-batch": "^1.0.5",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.0.7",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-watch": "^4.3.5",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
